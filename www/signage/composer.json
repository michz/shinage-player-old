{
  "name": "mztx/shinage-player",
  "license": "MIT",
  "type": "project",
  "description": "A php based in-browser-running player for shinage digital signage applications.",
  "require": {
    "php": ">=7.0",
    "mztx/shinage-player-bundle": "dev-master",
    "guzzlehttp/guzzle": "@stable",
    "symfony/console": "@stable",
    "symfony/framework-bundle": "@stable",
    "symfony/http-kernel": "@stable",
    "symfony/property-access": "@stable",
    "symfony/routing": "@stable",
    "symfony/serializer": "@stable",
    "symfony/templating": "@stable",
    "symfony/yaml": "@stable",
    "symfony/twig-bundle": "^3.3",
    "symfony/asset": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "symfony/web-server-bundle": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "mztx\\ShinageDiagnosticsBundle\\": "src/ShinageDiagnosticsBundle/",
      "": "src/"
    },
    "classmap": [ "app/AppKernel.php" ]
  }
}
